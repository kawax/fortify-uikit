{
    "name": "revolution/fortify-uikit",
    "description": "Laravel Fortify views for UIkit3",
    "keywords": [
        "laravel",
        "fortify",
        "uikit"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3||^8.0",
        "illuminate/support": "^8.0",
        "laravel/fortify": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Revolution\\Fortify\\Uikit\\": "src/"
        }
    },
    "authors": [
        {
            "name": "kawax",
            "email": "kawaxbiz@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Revolution\\Fortify\\Uikit\\UikitServiceProvider"
            ]
        }
    },
    "suggest": {
        "revolution/laravel-pagination-uikit": "Pagination views."
    }
}
